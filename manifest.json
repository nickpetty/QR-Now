{
  "manifest_version": 2,

  "name": "QR Now",
  "description": "Generates QR code for current tab.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "icons": {"16":"icon16.png",
            "48":"icon48.png",
            "128":"icon128.png"}
}
